{
    "manifest_version": 3,
    "name": "Video Transcription Extension",
    "version": "1.0",
    "description": "Transcribe YouTube videos in real-time",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  